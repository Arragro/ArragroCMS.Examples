  "description": "CMS for www.arragro.com using ArragroCMS",
  "main": "index.js",
  "scripts": {
    "build-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --progress --profile --colors",
    "build-prod": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --env.prod --progress --profile --colors --display-error-details",
    "run-app": "npm install && npm run build-prod && dotnet run -c Release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
