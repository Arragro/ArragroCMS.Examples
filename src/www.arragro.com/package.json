{
  "name": "www.arragro.com",
  "version": "1.0.0",
  "description": "Web front end for presenting the ArragroCMS content",
  "main": "index.js",
  "scripts": {
    "build-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --progress --profile",
    "build-prod": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --progress --profile --run-prod",
    "run-app": "npm install && npm run build-prod && dotnet run -c Release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arragro/ArragroCMS.Examples.git"
  },
  "keywords": [
    "dotnet",
    "core",
    "cms"
  ],
  "author": "Michael Jewell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arragro/ArragroCMS.Examples/issues"
  },
  "homepage": "https://github.com/Arragro/ArragroCMS.Examples#readme",
