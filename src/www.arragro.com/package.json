{
  "name": "www.arragro.com",
  "version": "1.0.0",
  "description": "Web front end for presenting the ArragroCMS content",
  "main": "index.js",
  "scripts": {
    "build-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --progress --profile --colors",
    "build-prod": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --env.prod --progress --profile --colors --display-error-details",
    "run-app": "npm install && npm run build-prod && dotnet run -c Release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arragro/ArragroCMS.Examples.git"
  },
  "keywords": [
    "dotnet",
    "core",
    "cms"
  ],
  "author": "Michael Jewell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Arragro/ArragroCMS.Examples/issues"
  },
  "homepage": "https://github.com/Arragro/ArragroCMS.Examples#readme",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "popper.js": "^1.12.9",
    "jquery": "^3.3.1",
    "jquery-validation": "^1.17.0",
    "jquery-validation-unobtrusive": "^3.2.6",
    "gsap": "^1.20.4"
  },
  "browserslist ": [
    "last 2 versions"
  ],
  "devDependencies": {
    "@types/gsap": "^1.19.2",
    "@types/source-map": "^0.5.7",
    "@types/uglify-js": "^2.6.30",
    "@types/webpack": "^3.0.14",
    "@types/webpack-env": "^1.13.5",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^2.0.3",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "compression-webpack-plugin": "^1.1.10",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "glob-all": "^3.1.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.1",
    "precss": "^3.1.2",
